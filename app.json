{
  "expo": {
    "name": "OnSite Timekeeper",
    "slug": "onsite-timekeeper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "onsite-timekeeper",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0B0D10",
    "newArchEnabled": false,
   
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onsiteclub.timekeeper",
      "config": {
        "googleMapsApiKey": "AIzaSyBdITLu3Kr5OYPr8ZnhxIGVNFHIGEgvfKY"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "OnSite Timekeeper needs your location to show where you are on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "OnSite Timekeeper needs background location access to automatically detect when you arrive at or leave work.",
        "NSLocationAlwaysUsageDescription": "OnSite Timekeeper needs background location access to automatically track your work hours.",
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification",
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0D10"
